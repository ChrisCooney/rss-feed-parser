{
    "rules": {
        "no-console": 0,
        "indent": 0,
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "never"
        ]
    },
    "env": {
        "es6": true,
        "node": true,
        "jest/globals": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "modules": true,
        "sourceType": "module"
    },
    "plugins": ["jest"],
    "extends": ["eslint:recommended", "plugin:jest/recommended"]
}
